            <intent-filter>
                <action android:name="com.koreaaaa2gmail.actualproject.Setting1" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".ChangePassword"
            android:label="Change Password">
            <intent-filter>
                <action android:name="com.koreaaaa2gmail.actualproject.ChangePassword" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>